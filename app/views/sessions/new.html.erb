<h2>Welcome Back!</h2>
<h3>Sign In</h3>

<%= link_to "Sign up!", new_user_url %>

<br><br>

<form action="<%=session_url%>" method="post">
  <%= auth_token %>

  <label>Email:
    <input type="text" name="user[email]">
  </label>

  <br>

  <label>Password:
    <input type="password" name="user[password]">
  </label>

  <br>

  <input type="submit" value="Sign In">
</form>
